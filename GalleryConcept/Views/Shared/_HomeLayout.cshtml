<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - GalleryConcept</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
</head>
<body>
<div class="container p-4">
    <h1 class="display-1 mb-4" style="text-transform: uppercase">Tymczasowa<br> wystawa<br> o kolorze,<br> którego nie<br> ma w tęczy</h1>
    <nav class="navbar navbar-light bg-white box-shadow mb-4">
        <ul class="navbar-nav flex-grow-1">
            <li class="nav-item">
                <a class="nav-link text-dark fw-bold fs-2" asp-area="" asp-controller="Home" asp-action="">o wystawie</a>
            </li>
            <li class="nav-item">
                <a class="nav-link text-dark fw-bold fs-2" asp-area="" asp-controller="Home" asp-action="">e-katalog</a>
            </li>
            <li class="nav-item">
                <a class="nav-link text-dark fw-bold fs-2" asp-area="" asp-controller="Home" asp-action="">kontakt</a>
            </li>
        </ul>
    </nav>
    <main role="main" class="pb-3">
        @RenderBody()
    </main>
</div>

    @* <footer class="border-top footer text-muted"> *@
    @*     <div class="container"> *@
    @*         &copy; 2022 - GalleryConcept - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a> *@
    @*     </div> *@
    @* </footer> *@
    <script src="https://code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
